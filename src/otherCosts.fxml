<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.collections.FXCollections?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="600.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/8.0.171"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="sellingController">
    <children>
        <Text layoutX="447.0" layoutY="152.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Select Date : "/>
        <DatePicker fx:id="OC_date" layoutX="549.0" layoutY="135.0" promptText="MM/DD/YYYY"/>
        <Text layoutX="238.0" layoutY="228.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Enter Supplier ID"/>
        <Text layoutX="238.0" layoutY="266.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Supplier Name :"/>
        <Text fx:id="sup_name" layoutX="373.0" layoutY="266.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Name"
              wrappingWidth="189.404296875"/>
        <Text layoutX="238.0" layoutY="313.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Enter Type :"/>
        <ComboBox fx:id="OC_type" layoutX="373.0" layoutY="296.0" prefWidth="150.0" promptText="Select Type">
            <items>
                <FXCollections fx:factory="observableArrayList">
                    <String fx:value="Stamp Duty"/>
                    <String fx:value="Stationery"/>
                    <String fx:value="Other"/>
                </FXCollections>
            </items>
        </ComboBox>
        <Button fx:id="OC_add_btn" layoutX="598.0" layoutY="441.0" mnemonicParsing="false" onAction="#AddOtherCost"
                prefHeight="25.0" prefWidth="125.0" text="ADD"/>
        <Button fx:id="OC_reset_btn" layoutX="400.0" layoutY="441.0" mnemonicParsing="false" onAction="#Reset"
                prefHeight="25.0" prefWidth="125.0" text="Reset"/>
        <TextField fx:id="OC_amount" layoutX="373.0" layoutY="341.0" promptText="Rs"/>
        <Button fx:id="OC_backtoHm_btn" layoutX="14.0" layoutY="361.0" mnemonicParsing="false" text="Home"
                AnchorPane.bottomAnchor="14.0" AnchorPane.leftAnchor="14.0" onAction="#BackToHome"/>
        <Text layoutX="238.0" layoutY="358.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Amount : "/>
        <CheckBox fx:id="OC_paid" layoutX="652.0" layoutY="391.0" mnemonicParsing="false" prefHeight="17.0"
                  prefWidth="69.0" text="Paid" textAlignment="CENTER"/>
        <ComboBox fx:id="sup_id" layoutX="372.0" layoutY="211.0" prefWidth="150.0" promptText="Enter ID" onAction="#SetName"/>
    </children>
</AnchorPane>
