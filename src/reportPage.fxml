<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<?import java.lang.String?>
<?import javafx.collections.FXCollections?>
<AnchorPane prefHeight="600.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/8.0.171"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="ReportPage">
    <children>
        <Button fx:id="htmlbtn" layoutX="500.0" layoutY="540.0" mnemonicParsing="false" onAction="#HtmlBtn"
                text="HTML"/>
        <Text layoutX="102.0" layoutY="136.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Select Supply ID"/>
        <Text layoutX="102.0" layoutY="170.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Name : "/>
        <Text fx:id="single_sup_name" layoutX="218.0" layoutY="169.0" strokeType="OUTSIDE" strokeWidth="0.0"
              text="Name"/>
        <Text layoutX="102.0" layoutY="216.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Enter Year :"/>
        <Text layoutX="102.0" layoutY="267.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Select Month :"/>
        <ComboBox fx:id="single_sup_id" layoutX="218.0" layoutY="119.0" onAction="#SetName" prefWidth="150.0"
                  promptText="Select ID"/>
        <TextField fx:id="single_sup_year" layoutX="219.0" layoutY="199.0"/>
        <ComboBox fx:id="single_sup_month" layoutX="218.0" layoutY="250.0" prefWidth="150.0" promptText="Select Month">
            <items>
                <FXCollections fx:factory="observableArrayList">
                    <String fx:value="January" />
                    <String fx:value="February" />
                    <String fx:value="March" />
                    <String fx:value="April" />
                    <String fx:value="May" />
                    <String fx:value="June" />
                    <String fx:value="July" />
                    <String fx:value="August" />
                    <String fx:value="September " />
                    <String fx:value="October" />
                    <String fx:value="November" />
                    <String fx:value="December" />
                </FXCollections>
            </items>
        </ComboBox>
        <Button fx:id="single_sup_report_gen" layoutX="264.0" layoutY="298.0" mnemonicParsing="false"
                text="Generate Report"/>
        <Text layoutX="532.0" layoutY="205.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Enter Year :"/>
        <Text layoutX="532.0" layoutY="256.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Select Month :"/>
        <TextField fx:id="all_sup_year" layoutX="649.0" layoutY="188.0"/>
        <ComboBox fx:id="all_sup_month" layoutX="648.0" layoutY="239.0" prefWidth="150.0" promptText="Select Month"/>
        <Button fx:id="all_sup_report_gen" layoutX="694.0" layoutY="287.0" mnemonicParsing="false"
                text="Generate Report"/>
    </children>
</AnchorPane>
